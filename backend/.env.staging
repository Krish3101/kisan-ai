# Staging Environment Configuration
DEBUG=false
LOG_LEVEL=INFO
API_V1_STR=/api/v1

# Database (Update with actual staging DB)
DATABASE_URL=postgresql://user:password@staging-db:5432/kisanai

# Security (Generate new secret for staging)
SECRET_KEY=change-this-to-random-secret-for-staging
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# External APIs (Use staging/test API keys if available)
OPENWEATHER_API_KEY=your-openweather-api-key
INDIA_GOV_API_KEY=your-india-gov-api-key
OPENROUTER_API_KEY=your-openrouter-api-key

# CORS (Update with staging frontend URL)
BACKEND_CORS_ORIGINS=["https://staging.kisanai.com"]

# Cache
CACHE_TTL_WEATHER=21600
CACHE_TTL_PRICES=86400

# Monitoring (Optional)
SENTRY_DSN=your-sentry-dsn-for-staging
